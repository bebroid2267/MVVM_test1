                    xmlns:view="clr-namespace:MVVM_test1" xmlns:view1="clr-namespace:MVVM_test1.View" >

    <DataTemplate DataType="{x:Type vm:MainVM}">
        <view:MainWindow />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AllSoftVM}">
        <view1:AllSoft />
    </DataTemplate>

    
</ResourceDictionary>